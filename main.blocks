 Set the servo controller to run a actiongroup" line434="    * @param times Running times. eg: 1" line435="    */" line436="    //% weight=96 blockId=startbit_runActionGroup block=&quot;Run ActionGroup|index %index|times %times&quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line437="    export function startbit_runActionGroup(index: number, times: number = 1) {" line438="" line439="        let buf2 = pins.createBuffer(7);" line440="        buf2[0] = 0x55;" line441="        buf2[1] = 0x55;" line442="        buf2[2] = 0x05;" line443="        buf2[3] = 0x06;//cmd type CMD_ACTION_GROUP_RUN" line444="        buf2[4] = index &amp; 0xff;" line445="        buf2[5] = times &amp; 0xff;" line446="        buf2[6] = (times &gt;&gt; 8) &amp; 0xff;" line447="" line448="        actiongroup_finished = false;" line449="        serial.writeBuffer(buf2);" line450="    }" line451="&#9;" line452="    /**" line453="    * Stop running actiongroup" line454="    */" line455="    //% weight=95 blockId=startbit_stopnActionGroup block=&quot;Stop ActionGroup&quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line456="    export function startbit_stopActionGroup() {" line457="" line458="        let buf3 = pins.createBuffer(7);" line459="        buf3[0] = 0x55;" line460="        buf3[1] = 0x55;" line461="        buf3[2] = 0x02;" line462="        buf3[3] = 0x07;//cmd type CMD_ACTION_GROUP_STOP" line463="&#9;    " line464="&#9;actiongroup_finished = false;" line465="        serial.writeBuffer(buf3);" line466="    }" line467="&#9;" line468="    /**" line469="     * Wait for Actiongroup Finishing" line470="     */" line471="    //% weight=94 blockId=startbit_actionRunover block=&quot;Action run over&quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line472="    export function startbit_actionRunover(): boolean {" line473="        // let ret = false;" line474="        if (actiongroup_finished == true) {" line475="            // ret = true;" line476="            actiongroup_finished = true;" line477="        }" line478="        else {" line479="            actiongroup_finished = false;" line480="        }" line481="        return actiongroup_finished;" line482="    }" line483="" line484="&#9;" line485="    /**" line486="     * Send read startbit servos angle command" line487="     */" line488="    //% weight=92 blockId=startbit_readAngle block=&quot;Send |%servo|angle command &quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line489="    export function startbit_readAngle(servo: startbit_Servos) {" line490="        let buf4 = pins.createBuffer(6);" line491="        buf4[0] = 0x55;" line492="        buf4[1] = 0x55;" line493="        buf4[2] = 0x04;" line494="        buf4[3] = 0x3E;//cmd type" line495="        buf4[4] = 0x05;" line496="        buf4[5] = servo;" line497="        serial.writeBuffer(buf4);" line498="    }" line499="" line500="" line501="    /**" line502="     * Do someting when Startbit receive angle" line503="     * @param body code to run when event is raised" line504="     */" line505="    //% weight=90 blockId=onStartbit_getAngle block=&quot;on Startbit|%servo|get angle&quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line506="    export function onStartbit_getAngle(servo: startbit_Servos, body: Action) {" line507="        control.onEvent(MESSAGE_ANGLE, servo, body);" line508="    }" line509="" line510="" line511="    /**" line512="     *  Get servos angle" line513="     */" line514="    //% weight=88 blockId=getServosAngle blockGap=50 block=&quot;Get|%servo|angle(-120~120)&quot; group=&quot;2. Servo-Motor.&quot; advanced=true" line515="    export function getServosAngle(servo: startbit_Servos): number {" line516="        if (servo == startbit_Servos.Servo1) {" line517="            return servo1Angle;" line518="        }" line519="        else if (servo == startbit_Servos.Servo2) {" line520="            return servo2Angle;" line521="        }" line522="        else" line523="            return 0xFFF;" line524="    }" li