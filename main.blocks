  function setI2CAddress(): void {" line1492="        setcmd(0x33)" line1493="        basic.pause(5)" line1494="        send(0x30)" line1495="        basic.pause(5)" line1496="        send(0x20)" line1497="        basic.pause(5)" line1498="        setcmd(0x28)" line1499="        setcmd(0x0C)" line1500="        setcmd(0x06)" line1501="        setcmd(0x01)" line1502="    } " line1503="" line1504="    /**" line1505="     * Initialize the I2C address" line1506="     */" line1507="    //% blockId=&quot;LCD_setAddress&quot; block=&quot; Initialize LCD1602 I2C |%port address |%myAddr&quot; group=&quot;1. Declaration.&quot;" line1508="    //% weight=51 blockExternalInputs=true" line1509="    export function setAddress(port: startbit_LCDPort, myAddr: I2C_ADDR): void {" line1510="        LCD_I2C_ADDR = myAddr" line1511="        setI2CAddress()" line1512="    }" line1513="" line1514="    /**" line1515="     * Initialize the I2C address (number)" line1516="     */" line1517="   // //% blockId=&quot;LCD_setAddress2&quot; block=&quot;LCD1602 I2C address %myAddr&quot; group=&quot;8. Display&quot; " line1518="    //% weight=50 blockExternalInputs=true" line1519="    //export function setAddress2(myAddr: number): void {" line1520="    //    LCD_I2C_ADDR = myAddr" line1521="    //    setI2CAddress()" line1522="    //}" line1523="    /*" line1524="    function AutoAddr() {" line1525="        let k = true" line1526="        let addr = 0x20" line1527="        let d1 = 0, d2 = 0" line1528="        while (k &amp;&amp; (addr &lt; 0x28)) {" line1529="            pins.i2cWriteNumber(addr, -1, NumberFormat.Int32LE)" line1530="            d1 = pins.i2cReadNumber(addr, NumberFormat.Int8LE) % 16" line1531="            pins.i2cWriteNumber(addr, 0, NumberFormat.Int16LE)" line1532="            d2 = pins.i2cReadNumber(addr, NumberFormat.Int8LE)" line1533="            if ((d1 == 7) &amp;&amp; (d2 == 0)) k = false" line1534="            else addr += 1" line1535="        }" line1536="        if (!k) return addr" line1537="         addr = 0x38" line1538="        while (k &amp;&amp; (addr &lt; 0x40)) {" line1539="            pins.i2cWriteNumber(addr, -1, NumberFormat.Int32LE)" line1540="            d1 = pins.i2cReadNumber(addr, NumberFormat.Int8LE) % 16" line1541="            pins.i2cWriteNumber(addr, 0, NumberFormat.Int16LE)" line1542="            d2 = pins.i2cReadNumber(addr, NumberFormat.Int8LE)" line1543="            if ((d1 == 7) &amp;&amp; (d2 == 0)) k = false" line1544="            else addr += 1" line1545="        }" line1546="        if (!k) return addr" line1547="        else return 0" line1548="    }" line1549="    */" line1550="" line1551="    /**" line1552="     * Automatically initialize the I2C address" line1553="     */" line1554="    // //% blockId=&quot;LCD_setAddress3&quot; block=&quot;Auto set LCD1602 I2C address&quot; group=&quot;8. Display&quot; " line1555="    //% weight=50" line1556="    //export function setAddress3(): void {" line1557="     //   LCD_I2C_ADDR = AutoAddr()" line1558="     //   setI2CAddress()" line1559="    //}  " line1560="" line1561="    /**" line1562="     * LCD clear " line1563="     */" line1564="    //% blockId=&quot;LCD_clear&quot; block=&quot;LCD clear&quot; group=&quot;8. Display&quot; " line1565="    //% weight=45" line1566="    export function clear(): void {" line1567="        setcmd(0x01)" line1568="    }" line1569="   " line1570="    /**" line1571="     * Setting backlight" line1572="     */" line1573="    //% blockId=&quot;LCD_backlight&quot; block=&quot;set LCD backlight %on&quot; group=&quot;8. Display&quot;  advanced=true" line1574="    //% weight=46" line1575="    export function set_backlight(on: on_off): void {" line1576="        if (on == 1)" line1577="            BK = 0x08" line1578="        else" line1579="            BK = 0x00" line1580="        setcmd(0x00)" line1581="    }" line1582="" line1583="    /**" line1584="     * Show string." line1585="     */" line1586="    //% blockId=&quot;LCD_Show&quot; block=&quot;set string %show&quot; group=&quot;8. Display&quot; " line1587="    //% weight=47" line1588="    export function set_LCD_Show(show: visibled): void {" line1589="        if (show == 1)" line1590="            setcmd(0x0C)" line1591="        else" line1592="            setcmd(0x08)" line1593="    }" line1594="" line1595="    function printChar(ch: number, x: number, y: number): void {" line1596="        if (x &gt;= 0) {" line1597="            let a = 0x80" line1598="            if (y == 1)" line1599="                a = 0xC0" line1600="&#9;    if (y == 2)" line1601="                a = 0x80 + 0x14" line1602="            if (y == 3)" line1603="                a = 0xC0 + 0x14" line1604="            a += x" line1605="            setcmd(a)" line1606="        }" line1607="        setdat(ch)" line1608="    }" line1609="" line1610="    /**" line1611="     * Show x, y string " line1612="     */" line1613="    //% blockId=&quot;LCD_putString&quot; block=&quot;LCD show string %s|on x:%x|y:%y&quot; group=&quot;8. Display&quot;   advanced=true" line1614="    //% weight=49 blockExternalInputs=true x.min=0 x.max=15 y.min=0 y.max=1" line1615="    export function putString(s: string, x: number, y: number): void {" line1616="        if (s.length &gt; 0) {" line1617="            let breakPoint = -1" line1618="            printChar(s.charCodeAt(0), x, y)" line1619="            if (y == 0)" line1620="                breakPoint = 16 - x" line1621="            for (let i = 1; i &lt; s.length; i++) {" line1622="                if (i == breakPoint)" line1623="                    printChar(s.charCodeAt(i), 0, 1)" line1624="                else" line1625="                    printChar(s.charCodeAt(i), -1, 0)" line1626="            }" line1627="        }" line1628="    }" line1629="&#9;" line1630="    /**" line1631="     * Show number" line1632="     */" line1633="    //% blockId=&quot;LCD_putNumber&quot; block=&quot;LCD show number %n|on x:%x|y:%y&quot; group=&quot;8. Display&quot