    */" line1690="        _stop() {" line1691="            pins.digitalWritePin(this.dio, 0);" line1692="            pins.digitalWritePin(this.clk, 1);" line1693="            pins.digitalWritePin(this.dio, 1);" line1694="        }" line1695="" line1696="        /**" line1697="         * send command1" line1698="         */" line1699="        _write_data_cmd() {" line1700="            this._start();" line1701="            this._write_byte(TM1640_CMD1);" line1702="            this._stop();" line1703="        }" line1704="" line1705="        /**" line1706="         * send command3" line1707="         */" line1708="        _write_dsp_ctrl() {" line1709="            this._start();" line1710="            this._write_byte(TM1640_CMD3 | this._ON | this.brightness);" line1711="            this._stop();" line1712="        }" line1713="" line1714="        /**" line1715="         * send a byte to 2-wire interface" line1716="         */" line1717="        _write_byte(b: number) {" line1718="            for (let j = 0; j &lt; 8; j++) {" line1719="                pins.digitalWritePin(this.clk, 0);" line1720="                pins.digitalWritePin(this.dio, (b &gt;&gt; j) &amp; 1);" line1721="                pins.digitalWritePin(this.clk, 1);" line1722="" line1723="            }" line1724="            pins.digitalWritePin(this.clk, 1);" line1725="            pins.digitalWritePin(this.clk, 0);" line1726="        }" line1727="" line1728="        intensity(val: number = 7) {" line1729="            if (val &lt; 1) {" line1730="                this.off();" line1731="                return;" line1732="            }" line1733="            if (val &gt; 8) val = 8;" line1734="            this._ON = 8;" line1735="            this.brightness = val - 1;" line1736="            this._write_data_cmd();" line1737="            this._write_dsp_ctrl();" line1738="        }" line1739="" line1740="        /**" line1741="         * set data to TM1640, with given bit" line1742="         */" line1743="        _dat(bit: number, dat: number) {" line1744="            this._write_data_cmd();" line1745="            this._start();" line1746="            this._write_byte(TM1640_CMD2 | (bit % this.count))" line1747="            this._write_byte(dat);" line1748="            this._stop();" line1749="            this._write_dsp_ctrl();" line1750="        }" line1751="" line1752="" line1753="        showbit(num: number = 5, bit: number = 0) {" line1754="            this.buf[bit % this.count] = _SEGMENTS[num % 16]" line1755="            this._dat(bit, _SEGMENTS[num % 16])" line1756="        }" line1757="" line1758="        showNumber(num: number) {" line1759="            if (num &lt; 0) {" line1760="                this._dat(0, 0x40) // '-'" line1761="                num = -num" line1762="            }" line1763="            else" line1764="                this.showbit(Math.idiv(num, 1000) % 10)" line1765="            this.showbit(num % 10, 3)" line1766="            this.showbit(Math.idiv(num, 10) % 10, 2)" line1767="            this.showbit(Math.idiv(num, 100) % 10, 1)" line1768="        }" line1769="" line1770="        showHex(num: number) {" line1771="            if (num &lt; 0) {" line1772="                this._dat(0, 0x40) // '-'" line1773="                num = -num" line1774="            }" line1775="            else" line1776="                this.showbit((num &gt;&gt; 12) % 16)" line1777="            this.showbit(num % 16, 3)" line1778="            this.showbit((num &gt;&gt; 4) % 16, 2)" line1779="            this.showbit((num &gt;&gt; 8) % 16, 1)" line1780="        }" line1781="" line1782="" line1783="        showDP(bit: number = 1, show: boolean = true) {" line1784="            bit = bit % this.count" line1785="            if (show) this._dat(bit, this.buf[bit] | 0x80)" line1786="            else this._dat(bit, this.buf[bit] &amp; 0x7F)" line1787="        }" line1788="" line1789="        clear() {" line1790="            for (let k = 0; k &lt; this.count; k++) {" line1791="                this._dat(k, 0)" line1792="                this.buf[k] = 0" line1793="            }" line1794="        }" line1795="" line1796="        on() {" line1797="            this._ON = 8;" line1798="            this._write_data_cmd();" line1799="            this._write_dsp_ctrl();" line1800="        }" line1801="" line1802="        off() {" line1803="            this._ON = 0;" line1804="            this._write_data_cmd();" line1805="            this._write_dsp_ctrl();" line1806="        }" line1807="    }" line1808="    /**" line1809="     * TM 1640 Module." line1810="     * @param clk the CLK pin for TM1640, eg: DigitalPin.P1, P13" line1811="     * @param dio the DIO pin for TM1640, eg: DigitalPin.P2, P14" line1812="     * @param intensity the brightness of the LED, eg: 7" line1813="     * @param count the count of the LED, eg: 4" line1814="     */" line1815="    function startbit_TM1640create(port: startbit_digitaltubePort, intensity: number, count: number): startbit_TM1640LEDs {" line1816="        let digitaltube = new startbit_TM1640LEDs();" line1817="        switch (port) {" line1818="            case startbit_digitaltubePort.port1:" line1819="                digitaltube.clk = DigitalPin.P2;" line1820="                digitaltube.dio = DigitalPin.P1;" line1821="                break;" line1822="            case startbit_digitaltubePort.port2:" line1823="                digitaltube.clk = DigitalPin.P14;" line1824="                digitaltube.dio = DigitalPin.P13;" line1825="                break;" line1826="        }" line1827="" line1828="        if ((count &lt; 1) || (count &gt; 5)) count = 4;" line1829="        digitaltube.count = count;" line1830="        digitaltube.brightness = intensity;" line1831="        digitaltube.init();" line1832="        return digitaltube;" line1833="    }" line1834="" line1835="  /**" line1836="     * MT1640: Port 1: clk-P1, dio-P2 ; Port 2: clk-P13, dio-P14 ; " line1837="     * @param clk the CLK pin for TM1640, eg: DigitalPin.P1" line1838="     * @param dio the DIO pin for TM1640, eg: DigitalPin.P2" line1839="     * @param intensity the brightness of the LED, eg: 7" line1840="     * @param count the count of the LED, eg: 4" line1841="     */" line1842="    //% weight=77 blockId=startbit_digitaltube block=&quot;digitaltube|%port|intensity %intensity|LED count %count&quot; group=&quot;1. Declaration.&quot;  advanced=true " line1843="    export function startbit_digitaltube(port: startbit_digitaltubePort, intensity: number, count: number) { " line1844="        Digitaltube = startbit_TM1640create(port, intensity, count);" line1845="    }" line1846="" line1847="    /**" line1848="     * show a number. " line1849="     * @param num is a number, eg: 0" line1850="     */" line1851="    //% weight=76 blockId=startbit_showNumber block=&quot;digitaltube show number| %num&quot;  group=&quot;8. Display&quot;   advanced=true" line1852="    export function startbit_showNumber(num: number)  {" line1853="        Digitaltube.showNumber(num);" line1854="    }" line1855="" line1856="    /**" line1857="     * show a number in given position. " line1858="     * @param num number will show, eg: 5" line1859="     * @param bit the position of the LED, eg: 0" line1860="     */" line1861="    //% weight=75 blockId=startbit_showbit block=&quot;digitaltube show digit| %num|at %bit&quot;  group=&quot;8. Display&quot;   advanced=true" line1862="    export function startbit_showbit(num: number = 5, bit: number = 0) {" line1863="        Digitaltube.showbit(num, bit);" line1864="    }" line1865="" line1866="    /**" line1867="     * show a hex number. " line1868="     * @param num is a hex number, eg: 0" line1869="     */" line1870="    //% weight=74 blockId=startbit_showhex block=&quot;digitaltube show hex number| %num&quot;  group=&quot;8. Display&quot;   advanced=true" line1871="    export function startbit_showhex(num: number) {" line1872="        Digitaltube.showHex(num);" line1873="    }" line1874="" line1875="    /**" line1876="     * show or hide dot point. " line1877="     * @param bit is the position, eg: 1" line1878="     * @param show is show/hide dp, eg: true" line1879="     */" line1880="    //% weight=73 blockId=startbit_showDP block=&quot;digitaltube DotPoint at| %bit|show %show&quot;  group=&quot;8. Display&quot;   advanced=true" line1881="    export function startbit_showDP(bit: number = 1, show: boolean = true) {" line1882="        Digitaltube.showDP(bit, show);" line1883="    } " line1884="" line1885="    /**" line1886="     * set TM1640 intensity, range is [0-8], 0 is off." line1887="     * @param val the brightness of the TM1640, eg: 7" line1888="     */" line1889="    //% weight=72 blockId=startbit_intensity block=&quot; digitaltube set intensity %val&quot;  group=&quot;8. Display&quot;   advanced=true" line1890="    export function startbit_intensity(val: number = 7) {" line1891="        Digitaltube.intensity(val);" line1892="    } " line1893="" line1894="    /**" line1895="     * turn off LED. " line1896="     */" line1897="    //% weight=71 blockId=startbit_off block=&quot;turn off digitaltube&quot;  group=&quot;8. Display&quot;   advanced=true" line1898="    export function startbit_off() {" line1899="        Digitaltube.off();" line1900="    }" line1901="" line1902="    /**" line1903="     * turn on LED. " line1904="     */" line1905="    //% weight=70 blockId=startbit_on block=&quot;turn on digitaltube&quot;  group=&quot;8. Display&quot;   advanced=true" line1906="    export function startbit_on() {" line1907="        Digitaltube.on();" line1908="    }" line1909="" line1910="    /**" line1911="     * clear LED. " line1912="     */" line1913="    //%weight=69 blockId=startbit_clear blockGap=50 block=&quot;clear digitaltube&quot;  group=&quot;8. Display&quot;   advanced=true" line1914="    export function startbit_clear() {" line1915="        Digitaltube.clear();" line1916="    }  " line1917="}" numlines="1918"></mutation></block></statement></