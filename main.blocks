;" line619="    const APDS9960_PPULSE = 0x8E;" line620="    const APDS9960_CONTROL = 0x8F;" line621="    const APDS9960_CONFIG2 = 0x90;" line622="    const APDS9960_ID = 0x92;" line623="    const APDS9960_STATUS = 0x93;" line624="    const APDS9960_CDATAL = 0x94;" line625="    const APDS9960_CDATAH = 0x95;" line626="    const APDS9960_RDATAL = 0x96;" line627="    const APDS9960_RDATAH = 0x97;" line628="    const APDS9960_GDATAL = 0x98;" line629="    const APDS9960_GDATAH = 0x99;" line630="    const APDS9960_BDATAL = 0x9A;" line631="    const APDS9960_BDATAH = 0x9B;" line632="    const APDS9960_POFFSET_UR = 0x9D;" line633="    const APDS9960_POFFSET_DL = 0x9E;" line634="    const APDS9960_CONFIG3 = 0x9F;" line635="    const APDS9960_GCONF4 = 0xAB;" line636="    const APDS9960_AICLEAR = 0xE7;" line637="" line638="" line639="    /* LED Drive values */" line640="    const LED_DRIVE_100MA = 0;" line641="" line642="    /* ALS Gain (AGAIN) values */" line643="    const AGAIN_4X = 1;" line644="" line645="    /* Default values */" line646="    const DEFAULT_ATIME = 219;    // 103ms" line647="    const DEFAULT_WTIME = 246;    // 27ms" line648="    const DEFAULT_PROX_PPULSE = 0x87;    // 16us, 8 pulses" line649="    const DEFAULT_POFFSET_UR = 0;       // 0 offset" line650="    const DEFAULT_POFFSET_DL = 0;       // 0 offset      " line651="    const DEFAULT_CONFIG1 = 0x60;    // No 12x wait (WTIME) factor" line652="    const DEFAULT_AILT = 0xFFFF;  // Force interrupt for calibration" line653="    const DEFAULT_AIHT = 0;" line654="    const DEFAULT_PERS = 0x11;    // 2 consecutive prox or ALS for int." line655="    const DEFAULT_CONFIG2 = 0x01;    // No saturation interrupts or LED boost  " line656="    const DEFAULT_CONFIG3 = 0;       // Enable all photodiodes, no SAI" line657="    const DEFAULT_LDRIVE = LED_DRIVE_100MA;" line658="    const DEFAULT_AGAIN = AGAIN_4X;" line659="" line660="    const OFF = 0;" line661="    const POWER = 0;" line662="    const AMBIENT_LIGHT = 1;" line663="    const ALL = 7;" line664="" line665="    const red_wb = 2130;" line666="    const green_wb = 3500;" line667="    const blue_wb = 4620;" line668="" line669="    function i2cwrite(reg: number, value: number) {" line670="        let buf6 = pins.createBuffer(2);" line671="        buf6[0] = reg;" line672="        buf6[1] = value;" line673="        pins.i2cWriteBuffer(APDS9960_I2C_ADDR, buf6);" line674="    }" line675="" line676="    function i2cread(reg: number): number {" line677="        pins.i2cWriteNumber(APDS9960_I2C_ADDR, reg, NumberFormat.UInt8BE);" line678="        let val = pins.i2cReadNumber(APDS9960_I2C_ADDR, NumberFormat.UInt8BE);" line679="        return val;" line680="    }" line681="" line682="" line683="    function InitColor(): boolean {" line684="        let id = i2cread(APDS9960_ID);" line685="        //  serial.writeLine(&quot;id:&quot;)" line686="        //  serial.writeNumber(id); " line687="        // if (!(id == APDS9960_ID_1 || id == APDS9960_ID_2)) {" line688="        //     return false;" line689="        // }" line690="        //  serial.writeLine(&quot;set mode:&quot;)" line691="        setMode(ALL, OFF);" line692="        i2cwrite(APDS9960_ATIME, DEFAULT_ATIME);" line693="        i2cwrite(APDS9960_WTIME, DEFAULT_WTIME);" line694="        i2cwrite(APDS9960_PPULSE, DEFAULT_PROX_PPULSE);" line695="        i2cwrite(APDS9960_POFFSET_UR, DEFAULT_POFFSET_UR);" line696="        i2cwrite(APDS9960_POFFSET_DL, DEFAULT_POFFSET_DL);" line697="        i2cwrite(APDS9960_CONFIG1, DEFAULT_CONFIG1);" line698="        setLEDDrive(DEFAULT_LDRIVE);" line699="        setAmbientLightGain(DEFAULT_AGAIN);" line700="        setLightIntLowThreshold(DEFAULT_AILT);" line701="        setLightIntHighThreshold(DEFAULT_AIHT);" line702="        i2cwrite(APDS9960_PERS, DEFAULT_PERS);" line703="        i2cwrite(APDS9960_CONFIG2, DEFAULT_CONFIG2);" line704="        i2cwrite(APDS9960_CONFIG3, DEFAULT_CONFIG3);" line705="        return true;" line706="    }" line707="" line708="    function setLEDDrive(drive: number) {" line709="        let val2 = i2cread(APDS9960_CONTROL);" line710="        /* Set bits in register to given value */" line711="        drive &amp;= 0b00000011;" line712="        drive = drive &lt;&lt; 6;" line713="        val2 &amp;= 0b00111111;" line714="        val2 |= drive;" line715="        i2cwrite(APDS9960_CONTROL, val2);" line716="    }" line717="" line718="    function setLightIntLowThreshold(threshold: number) {" line719="        let val_low = threshold &amp; 0x00FF;" line720="        let val_high = (threshold &amp; 0xFF00) &gt;&gt; 8;" line721="        i2cwrite(APDS9960_AILTL, val_low);" line722="        i2cwrite(APDS9960_AILTH, val_high);" line723="    }" line724="" line725="    function setLightIntHighThreshold(threshold: number) {" line726="        let val_low2 = threshold &amp; 0x00FF;" line727="        let val_high2 = (threshold &amp; 0xFF00) &gt;&gt; 8;" line728="        i2cwrite(APDS9960_AIHTL, val_low2);" line729="        i2cwrite(APDS9960_AIHTH, val_high2);" line730="    }" line731="" line732="" line733="    function rgb2hue(r: number, g: number, b: number): number {" line734="        let max = Math.max(r, Math.max(g, b))" line735="        let min = Math.min(r, Math.min(g, b))" line736="        let c = max - min;" line737="        let hue = 0;" line738="        let segment = 0;" line739="        let shift = 0;" line740="        if (c == 0)" line741="            return 0;" line742="        if ((r &gt; g) &amp;&amp; (r &gt; b)) {" line743="            segment = (60.0 * (g - b)) / c;" line744="            if (segment &lt; 0)" line745="                hue = segment + 360;" line746="        }" line747="        else if ((g &gt; b) &amp;&amp; (g &gt; r)) {" line748="            segment = (60.0 * (b - r)) / c;" line749="            hue = segment + 120;" line750="        }" line751="        else if ((b &gt; g) &amp;&amp; (b &gt; r)) {" line752="            segment = (60.0 * (r - g)) / c;" line753="            hue = segment + 240;" line754="        }" line755="        return hue;" line756="    }" line757="" line758="    function setMode(mode: number, enable: number) {" line759="        let reg_val = getMode();" line760="        /* Change bit(s) in ENABLE register */" line761="        enable = enable &amp; 0x01;" line762="        if (mode &gt;= 0 &amp;&amp; mode &lt;= 6) {" line763="            if (enable &gt; 0) {" line764="                reg_val |= (1 &lt;&lt; mode);" line765="            }" line766="            else {" line767="                //reg_val &amp;= ~(1 &lt;&lt; mode);" line768="                reg_val &amp;= (0xff - (1 &lt;&lt; mode));" line769="            }" line770="        }" line771="        else if (mode == ALL) {" line772="            if (enable &gt; 0) {" line773="                reg_val = 0x7F;" line774="            }" line775="            else {" line776="                reg_val = 0x00;" line777="            }" line778="        }" line779="        i2cwrite(APDS9960_ENABLE, reg_val);" line780="    }" line781="" line782="    function getMode(): number {" line783="        let enable_value = i2cread(APDS9960_ENABLE);" line784="        return enable_value;" line785="    }" line786="" line787="    function enableLightSensor(interrupts: boolean) {" line788="        setAmbientLightGain(DEFAULT_AGAIN);" line789="        if (interrupts) {" line790="            setAmbientLightIntEnable(1);" line791="        }" line792="        else {" line793="            setAmbientLightIntEnable(0);" line794="        }" line795="        enablePower();" line796="        setMode(AMBIENT_LIGHT, 1);" line797="    }" line798="" line799="    function setAmbientLightGain(drive: number) {" line800="        let val3 = i2cread(APDS9960_CONTROL);" line801="        /* Set bits in register to given value */" line802="        drive &amp;= 0b00000011;" line803="        val3 &amp;= 0b11111100;" line804="        val3 |= drive;" line805="        i2cwrite(APDS9960_CONTROL, val3);" line806="    }" line807="" line808="    function getAmbientLightGain(): number {" line809="        let val4 = i2cread(APDS9960_CONTROL);" line810="        val4 &amp;= 0b00000011;" line811="        return val4;" line812="    }" line813="" line814="    function enablePower() {" line815="        setMode(POWER, 1);" line816="    }" line817="" line818="    function setAmbientLightIntEnable(enable: number) {" line819="        let val5 = i2cread(APDS9960_ENABLE);" line820="        /* Set bits in register to given value */" line821="        enable &amp;= 0b00000001;" line822="        enable = enable &lt;&lt; 4;" line823="        val5 &amp;= 0b11101111;" line824="        val5 |= enable;" line825="        i2cwrite(APDS9960_ENABLE, val5);" line826="    }" line827="" line828="    function readAmbientLight(): number {" line829="        let val6 = i2cread(APDS9960_CDATAL);" line830="        let val_byte = i2cread(APDS9960_CDATAH);" line831="        val6 = val6 + val_byte * 256;" line832="        return val6;" line833="    }" line834="" line835="" line836="    /**" line837="     * Initialize the color sensor,please execute at boot time" line838="     */" line839="    //% weight=87 blockId=startbit_init_colorSensor block=&quot;Initialize color sensor port at %port&quot; group=&quot;1. Declaration.&quot;" line840="    export function startbit_init_colorSensor(port: startbit_colorSensorPort) {" line841="        InitColor();" line842="        enableLightSensor(true);" line843="        control.waitMicros(100);" line844="    }" line845="" line846="    /**" line847="&#9; *  Color sensor return the color." line848="&#9; */" line849="    //% weight=86 blockId=startbit_checkCurrentColor block=&quot;Current color %color&quot; group=&quot;3. Sensor.&quot; " line850="    export function startbit_checkCurrentColor(color: startbit_Colors): boolean {" line851="        let d = i2cread(APDS9960_CDATAL) + i2cread(APDS9960_CDATAH) * 256;" line852="        let r = i2cread(APDS9960_RDATAL) + i2cread(APDS9960_RDATAH) * 256;" line853="        let g = i2cread(APDS9960_GDATAL) + i2cread(APDS9960_GDATAH) * 256;" line854="        let b = i2cread(APDS9960_BDATAL) + i2cread(APDS9960_BDATAH) * 256;" line855="" line856="        // serial.writeNumber(c);" line857="        // serial.writeLine(&quot;-&gt;ccc&quot;);" line858="        // serial.writeNumber(r);" line859="        // serial.writeLine(&quot;-&gt;red&quot;);" line860="        // serial.writeNumber(g);" line861="        // serial.writeLine(&quot;-&gt;green&quot;);" line862="        // serial.writeNumber(b);" line863="        // serial.writeLine(&quot;-&gt;blue&quot;);" line864="" line865="        if (r &gt; red_wb)" line866="            r = red_wb;" line867="        if (g &gt; green_wb)" line868="            g = green_wb;" line869="        if (b &gt; blue_wb)" line870="            b = blue_wb;" line871="" line872="        r = Math.round(mapRGB(r, 0, red_wb, 0, 255));" line873="        g = Math.round(mapRGB(g, 0, green_wb, 0, 255));" line874="        b = Math.round(mapRGB(b, 0, blue_wb, 0, 255));" line875="        // serial.writeNumber(r);" line876="        // serial.writeLine(&quot;-&gt;rred&quot;);" line877="        // serial.writeNumber(g);" line878="        // serial.writeLine(&quot;-&gt;ggreen&quot;);" line879="        // serial.writeNumber(b);" line880="        // serial.writeLine(&quot;-&gt;bblue&quot;);" line881="         let hsv = rgb2hue(r, g, b);" line882="        // serial.writeNumber(hsv);" line883="        // serial.writeLine(&quot;-&gt;hsv&quot;);" line884="        let t = startbit_Colors.None;" line885="        if (d &gt; 2200 &amp;&amp; r &gt; 65 &amp;&amp; g &gt; 65 &amp;&amp; b &gt; 65) {" line886="            t = startbit_Colors.White;" line887="        }" line888="        else if (d &gt; 800) {" line889="            if (hsv &lt; 8 || hsv &gt; 350)" line890="                t = startbit_Colors.Red;" line891="            else if (hsv &gt; 60 &amp;&amp; hsv &lt; 170) {" line892="                t = startbit_Colors.Green;" line893="            }" line894="            else if (hsv &gt; 210 &amp;&amp; hsv &lt; 230) {" line895="                t = startbit_Colors.Blue;" line896="            }" line897="        }" line898="        else if (d &gt; 200 &amp;&amp; r &gt; 10 &amp;&amp; g &gt; 7 &amp;&amp; b &gt; 7 &amp;&amp; r &lt; 16.5 &amp;&amp; g &lt; 15 &amp;&amp; b &lt; 14) {" line899="            t = startbit_Colors.Black;" line900="        }" line901="        return (color == t);" line902="    }" line903="    /**" line904="    * Get the volume level detected by the sound sensor, range 0 to 255" line905="    */" line906="    //% weight=82 blockId=startbit_getSoundVolume block=&quot;Sound volume&quot; group=&quot;3. Sensor.&quot;" line907="    export function startbit_getSoundVolume(): number {" line908="        return volume;" line909="    }" line910="" line911="    /**" line912="    * Get the obstacle avoidance sensor status,1 detect obstacle,0 no detect obstacle" line913="    * Pin 1 with Port 1" line914="    * Pin 13 with Port 2" line915="    * Pin ADC with Port 3" line916="    */" line917="    //% weight=85 blockId=startbit_avoidSensor block=&quot;Obstacle avoidance sensor|port %port|detect obstacle&quot; group=&quot;3. Sensor.&quot;" line918="    export function startbit_avoidSensor(port: startbit_touchKeyPort): boolean {" line919="        let status = 0;" line920="        let flag: boolean = false;" line921="        switch (port) {" line922="            case startbit_touchKeyPort.port1:" line923="                pins.setPull(DigitalPin.P1, PinPullMode.PullUp);" line924="                status = pins.digitalReadPin(DigitalPin.P1);" line925="                break;" line926="            case startbit_touchKeyPort.port2:" line927="                pins.setPull(DigitalPin.P13, PinPullMode.PullUp);" line928="                status = pins.digitalReadPin(DigitalPin.P13);" line929="                break;" line930="            case startbit_touchKeyPort.port3:" line931="   